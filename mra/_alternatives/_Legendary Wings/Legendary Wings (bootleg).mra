        <part name="ic18.bin" crc="2a00cde8"/>
        <part name="ic19.bin" crc="ec5cc201"/>
        <!-- CPU: non banked -->
        <part name="ic17.bin" crc="fe8a8823"/>

        <!-- Sound -->
        <part name="ic37.bin" crc="a20337a2"/>
        <!-- CHAR -->
        <part name="ic60.bin" crc="091d923c"/>
