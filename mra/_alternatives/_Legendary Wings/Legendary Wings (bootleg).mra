        <interleave output="16">
            <part name="ic99.bin" crc="163946da" map="01"/>
            <part name="ic87.bin" crc="bca275ac" map="10"/>
        </interleave>
        <interleave output="16">
            <part name="ic98.bin" crc="7cc90a1d" map="01"/>
